{
    "version": 2,
    "builds": [
        {
            "src": "./roughpage.exe",
            "use": "@vercel/static-build",
            "config": {
                "maxLambdaSize": "50mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "./public"
        }
    ],
    "cleanUrls": true
}